{
  "name": "nixelo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ],
    "*.json": [
      "biome format --write"
    ]
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "build": "convex codegen --typecheck disable && vite build",
    "biome": "biome check --write .",
    "biome:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "node node_modules/typescript/bin/tsc -b . --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:convex": "vitest --config vitest.convex.config.ts",
    "test:convex:ui": "vitest --config vitest.convex.config.ts --ui",
    "test:convex:coverage": "vitest --config vitest.convex.config.ts --coverage",
    "test:all": "pnpm run test run && pnpm run test:convex run",
    "check:api": "node scripts/check-api-calls.cjs",
    "check:queries": "node scripts/find-query-issues.cjs",
    "check:standards": "node scripts/check-standards.js",
    "generate:routes": "tsr generate",
    "check": "pnpm run typecheck && pnpm run biome:check && pnpm run check:api && pnpm run check:standards && pnpm run test:all",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:setup-auth": "node node_modules/tsx/dist/cli.mjs e2e/setup-auth.ts",
    "kill-port": "node scripts/kill-port.cjs 5555",
    "build:e2e": "vite build --mode e2e",
    "preview": "vite preview",
    "preview:e2e": "vite preview --mode e2e",
    "prepare": "husky",
    "dev:purge": "convex run purge:purgeData \"{\\\"confirm\\\": true}\"",
    "mirror": "node scripts/scrape_full_mirror.js",
    "mirror:batch": "node scripts/mirror_batch.js"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@auth/core": "^0.41.1",
    "@blocknote/core": "^0.45.0",
    "@blocknote/mantine": "^0.45.0",
    "@blocknote/react": "^0.45.0",
    "@convex-dev/action-cache": "^0.3.0",
    "@convex-dev/aggregate": "^0.2.1",

{
  "name": "nixelo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ],
    "*.{json,md,css}": [
      "biome format --write"
    ]
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "build": "vite build",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc -p convex -noEmit && tsc -p . -noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:convex": "vitest --config vitest.convex.config.ts",
    "test:convex:ui": "vitest --config vitest.convex.config.ts --ui",
    "test:convex:coverage": "vitest --config vitest.convex.config.ts --coverage",
    "test:all": "pnpm run test run && pnpm run test:convex run",
    "check:api": "node scripts/check-api-calls.cjs",
    "check": "pnpm run typecheck && pnpm run lint:check && pnpm run check:api && pnpm run test:all",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:setup-auth": "npx tsx e2e/setup-auth.ts",
    "preview": "vite preview",
    "prepare": "husky"
  },

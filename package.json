{
  "name": "nixelo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ],
    "*.json": [
      "biome format --write"
    ]
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "build": "convex codegen --typecheck disable && vite build",
    "biome": "biome check --write .",
    "biome:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "node node_modules/typescript/bin/tsc -b . --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:convex": "vitest --config vitest.convex.config.ts",
    "test:convex:ui": "vitest --config vitest.convex.config.ts --ui",
    "test:convex:coverage": "vitest --config vitest.convex.config.ts --coverage",
    "test:all": "npm-run-all test:run test:convex:run",
    "test:run": "vitest run",
    "test:convex:run": "vitest --config vitest.convex.config.ts run",
    "validate": "node scripts/validate.js",
    "generate:routes": "tsr generate",
    "check": "npm-run-all typecheck biome:check validate test:all",
    "fixme": "npm-run-all biome typecheck",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:setup-auth": "node node_modules/tsx/dist/cli.mjs e2e/setup-auth.ts",
    "screenshots": "node --env-file=.env.local node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/dist/cli.mjs e2e/screenshot-pages.ts",
    "screenshots:light-mode": "node --env-file=.env.local node_modules/.pnpm/tsx@4.21.0/node_modules/tsx/dist/cli.mjs e2e/screenshot-pages.ts --light",
    "kill-port": "node scripts/kill-port.cjs 5555",
    "build:e2e": "vite build --mode e2e",
    "preview": "vite preview",
    "preview:e2e": "vite preview --mode e2e",
    "prepare": "husky",
    "dev:purge": "convex run purge:purgeData \"{\\\"confirm\\\": true}\"",
    "mirror": "node scripts/scrape_full_mirror.js",
    "mirror:batch": "node scripts/mirror_batch.js",
    "inventory": "node scripts/check_inventory.js",
    "discover": "node scripts/discover_targets.js",
    "crawl": "node scripts/crawl_routes.js",
    "crawl:batch": "node scripts/crawl_batch.js",
    "select:targets": "node scripts/select_targets.js",
    "analyze": "node scripts/analyze_design.js",
    "setup:auth": "node scripts/setup_auth.js",
    "auth:manual": "node scripts/manual_handshake.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },

    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "react": "^19.2.3",
      "react-dom": "^19.2.3"
    }
  }
}
